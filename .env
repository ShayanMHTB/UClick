# UClick Environment Variables Template
# Copy this file to .env and fill in your actual values

# ===========================================
# SUPABASE CONFIGURATION
# ===========================================
# Get these from your Supabase project dashboard
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# ===========================================
# STRIPE PAYMENT CONFIGURATION
# ===========================================
# Get these from your Stripe dashboard
VITE_STRIPE_PUBLIC_KEY=pk_test_your-stripe-public-key-here
# Note: Secret key should never be in frontend, handle via Supabase Edge Functions

# ===========================================
# GOOGLE MAPS CONFIGURATION
# ===========================================
# Get this from Google Cloud Console
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# ===========================================
# PUSH NOTIFICATIONS CONFIGURATION
# ===========================================
# Get this from OneSignal dashboard (optional)
VITE_ONESIGNAL_APP_ID=your-onesignal-app-id-here

# ===========================================
# DEVELOPMENT CONFIGURATION
# ===========================================
# Environment mode
VITE_NODE_ENV=development

# API Base URL (for any custom APIs)
VITE_API_BASE_URL=http://localhost:3000/api

# ===========================================
# ANALYTICS & MONITORING (Optional)
# ===========================================
# Google Analytics (optional)
VITE_GA_TRACKING_ID=GA-XXXXXXXXX

# Sentry Error Tracking (optional)
VITE_SENTRY_DSN=https://your-sentry-dsn-here

# ===========================================
# FEATURE FLAGS (Optional)
# ===========================================
# Enable/disable features during development
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=false

# ===========================================
# DEBUGGING
# ===========================================
# Enable debug mode for additional logging
VITE_DEBUG_MODE=true
